{
    "name": "@baidu/boxx",
    "version": "0.0.4",
    "main": "dist/boxx.umd.js",
    "production": "dist/boxx.min.js",
    "module": "dist/boxx.esm.js",
    "commonjs": "dist/boxx.cjs.js",
    "devDependencies": {
        "vuepress": "^1.0.2"
    },
    "scripts": {
        "dev": "rollup --config build/rollup.config.dev.js -w --environment NODE_ENV:development ",
        "dev:mock": "rollup --config build/rollup.config.dev.js -w --environment NODE_ENV:development-mock ",
        "build": "node build/build.js",
        "deploy": "yarn build && fis3 release dev",
        "deploy:test": "yarn build && fis3 release test",
        "deploy:doc": "vuepress build doc && fis3 release doc",
        "doc": "vuepress dev doc",
        "doc:build": "vuepress build doc",
        "upload": "yarn build && node scripts/upload.js",
        "release": "yarn build && npm publish --registry=http://registry.npm.baidu-int.com && node scripts/upload.js"
    },
    "files": [
        "dist"
    ],
    "repository": {
        "type": "git",
        "url": "ssh://zhoudan03@icode.baidu.com:8235/baidu/search-fe/boxx"
    },
